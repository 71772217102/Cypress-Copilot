{
  "name": "cypress-copilot",
  "displayName": "Cypress Copilot",
  "publisher": "your-publisher",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:cypress-copilot.generateTest"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "vscode": "^1.1.65"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "openai": "^4.0.0",
    "node-fetch": "^3.3.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "cypress-copilot.generateTest",
        "title": "Cypress Copilot: Generate Test"
      }
    ]
  }
}
